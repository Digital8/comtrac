{
  "name": "comtrac",
  "version": "0.0.0",
  "private": true,
  "description": "The COMtrac website.",
  "main": "index.js",
  "devDependencies": {
    "ecstatic": "~0.4.12",
    "watchify": "~0.3.0",
    "css-combine": "0.0.6",
    "browserify": "~2.35.2",
    "uglify-js": "~2.4.3",
    "clean-css": "~2.0.1",
    "getport": "~0.1.0"
  },
  "scripts": {
    "start": "npm run watch-js & node server.js",
    "build": "npm run build-css && npm run build-js",
    "watch-js": "watchify assets/js/index.js -vo assets/js/build.js",
    "build-js": "browserify assets/js/index.js | uglifyjs -cmo assets/js/build.js > /dev/null 2>&1",
    "build-css": "css-combine assets/css/all.css | cleancss > assets/css/build.css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Digital8/comtrac.git"
  },
  "keywords": [
    "standby",
    "golf",
    "mobile"
  ],
  "bugs": {
    "url": "https://github.com/Digital8/comtrac/issues"
  }
}
